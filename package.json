{
  "name": "@renyuneyun/solid-helper",
  "version": "0.1.2",
  "description": "Common helpers for developing SoLiD (Social Linked Data) under JS/TS",
  "main": "./dist/solid-helper.umd.js",
  "module": "./dist/solid-helper.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/solid-helper.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/solid-helper.umd.js"
      }
    }
  },
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "serve": "vite preview",
    "test": "mocha",
    "test-e2e": "mocha test/e2e/",
    "test-all": "mocha --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/renyuneyun/solid-helper.git"
  },
  "keywords": [
    "SoLiD"
  ],
  "author": "Rui Zhao (renyuneyun)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/renyuneyun/solid-helper/issues"
  },
  "homepage": "https://github.com/renyuneyun/solid-helper#readme",
  "dependencies": {
    "@comunica/query-sparql": "^2.10.0",
    "@inrupt/vocab-common-rdf": "^1.0.5",
    "@renyuneyun/parse-link-header-ts": "^2.0.0",
    "solid-namespace": "^0.5.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.1.0",
    "mocha": "^10.3.0",
    "rollup-plugin-node-externals": "^7.0.1",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^3.0.0-beta.1",
    "vue-tsc": "^1.4.2"
  }
}
